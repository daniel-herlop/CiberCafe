<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary">

    <ListView
        android:id="@+id/listaAnularReservas"
        android:layout_width="350dp"
        android:layout_height="500dp"
        android:layout_marginTop="50dp"
        android:divider="@android:color/transparent"
        android:dividerHeight="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonAnular"
        android:layout_width="240dp"
        android:layout_height="60dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="@color/icons"
        android:onClick="anular"
        android:text="@string/anularSeleccionadas"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/listaAnularReservas"
        app:layout_constraintTop_toBottomOf="@+id/listaAnularReservas" />
</androidx.constraintlayout.widget.ConstraintLayout>