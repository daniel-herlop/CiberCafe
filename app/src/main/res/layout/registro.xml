<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:gravity="center"
        android:text="*"
        android:textSize="18dp"
        android:textColor="@color/primary_text"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="44dp"
        android:text="@string/usuario"
        android:textSize="18dp"
        android:textColor="@color/primary_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/contraseÃ±a"
        android:textSize="18dp"
        android:textColor="@color/primary_text"
        app:layout_constraintStart_toStartOf="@+id/entradaUsuario"
        app:layout_constraintTop_toBottomOf="@+id/entradaUsuario" />

    <EditText
        android:id="@+id/entradaUsuario"
        android:layout_width="280dp"
        android:layout_height="37dp"
        android:backgroundTint="@color/primary_text"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="20"
        android:textColor="@color/primary_text"
        android:textSize="18dp"
        android:textCursorDrawable="@null"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <EditText
        android:id="@+id/entradaPassw"
        android:layout_width="280dp"
        android:layout_height="37dp"
        android:backgroundTint="@color/primary_text"
        android:ems="10"
        android:inputType="textPassword"
        android:maxLength="20"
        android:textColor="@color/primary_text"
        android:textSize="18dp"
        android:textCursorDrawable="@null"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_marginTop="20dp"
        android:onClick="registrarUsuario"
        android:text="@string/confirmar"
        android:textSize="16dp"
        android:backgroundTint="@color/icons"
        app:layout_constraintStart_toStartOf="@+id/entradaEmail"
        app:layout_constraintTop_toBottomOf="@+id/entradaEmail" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/nombre"
        android:textSize="18dp"
        android:textColor="@color/primary_text"
        app:layout_constraintStart_toStartOf="@+id/entradaPassw"
        app:layout_constraintTop_toBottomOf="@+id/entradaPassw" />

    <EditText
        android:id="@+id/entradaNombre"
        android:layout_width="280dp"
        android:layout_height="37dp"
        android:backgroundTint="@color/primary_text"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="20"
        android:textColor="@color/primary_text"
        android:textSize="18dp"
        android:textCursorDrawable="@null"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/apellidos"
        android:textSize="18dp"
        android:textColor="@color/primary_text"
        app:layout_constraintStart_toStartOf="@+id/entradaNombre"
        app:layout_constraintTop_toBottomOf="@+id/entradaNombre" />

    <EditText
        android:id="@+id/entradaApellidos"
        android:layout_width="280dp"
        android:layout_height="37dp"
        android:backgroundTint="@color/primary_text"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="20"
        android:textColor="@color/primary_text"
        android:textSize="18dp"
        android:textCursorDrawable="@null"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/telefono"
        android:textSize="18dp"
        android:textColor="@color/primary_text"
        app:layout_constraintStart_toStartOf="@+id/entradaApellidos"
        app:layout_constraintTop_toBottomOf="@+id/entradaApellidos" />

    <EditText
        android:id="@+id/entradaTelefono"
        android:layout_width="280dp"
        android:layout_height="40dp"
        android:backgroundTint="@color/primary_text"
        android:ems="10"
        android:inputType="phone"
        android:maxLength="9"
        android:textColor="@color/primary_text"
        android:textCursorDrawable="@null"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/mail"
        android:textSize="18dp"
        android:textCursorDrawable="@null"
        android:textColor="@color/primary_text"
        app:layout_constraintStart_toStartOf="@+id/entradaTelefono"
        app:layout_constraintTop_toBottomOf="@+id/entradaTelefono" />

    <EditText
        android:id="@+id/entradaEmail"
        android:layout_width="280dp"
        android:layout_height="37dp"
        android:backgroundTint="@color/primary_text"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:maxLength="30"
        android:textColor="@color/primary_text"
        android:textSize="18dp"
        android:textCursorDrawable="@null"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:gravity="center"
        android:text="*"
        android:textSize="18dp"
        android:textCursorDrawable="@null"
        android:textColor="@color/primary_text"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView" />
</androidx.constraintlayout.widget.ConstraintLayout>